<div class="well well-sm">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-8">
            <div class="form-group">
                <div class="col-md-10">
                    <div class="row">
                        <div class="col-sm-12">
                            {{ form_start(form, {'action': path('admin_equipe_selectSaison'), 'method': 'POST', 'attr' : {'class' : ''} }) }}
                            {{ form_errors(form) }}
                            <div class="input-group input-group-md">
                                <div class="input-group-btn">
                                    <button name="add" type="submit" class="btn bg-color-blue txt-color-white" tabindex="-1"><i class="fa fa-plus"></i> <strong>Ajouter une Equipe</strong></button>
                                    <button name="view" type="submit" class="btn bg-color-pinkDark txt-color-white" tabindex="-1"><i class="fas fa-eye"></i> <strong>Voir les Equipes</strong></button>
                                </div>
                                <div class="icon-addon addon-md">
                                    {{ form_widget(form.nomSaison) }}
                                    {{ form_errors(form.nomSaison) }}
                                    <label for="saison" class="glyphicon glyphicon-search" rel="tooltip" title="Select Saison" data-original-title="saison"></label>
                                </div>
                            </div>
                            {{ form_end(form) }}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>